#include "digiproces-common.h"

int main(void) {
    volatile uint32_t i = 0;
    
    system_init();
    gpio_init();
    leds_init();
    button_init();
    
    antenna_init();
    antenna_external();    

    IntMasterEnable();

    while(1) {
        GPIOPinWrite(BSP_LED_BASE, BSP_LED_GREEN, BSP_LED_GREEN);
        for(i = 0; i < WAIT_TICKS ; i++);
        GPIOPinWrite(BSP_LED_BASE, BSP_LED_GREEN, 0);
        for(i = 0; i < WAIT_TICKS ; i++);
    }

    return 0;
}

